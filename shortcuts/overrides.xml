<overrides>
	<alwaysReset>True</alwaysReset>
	<doNotShareMenu />

	<!-- action overrides -->
	<override action="start_search">
		<condition>!Skin.HasSetting(UseGlobalsearch)</condition>
		<action>SetProperty(CustomSearch,1,home)</action>
