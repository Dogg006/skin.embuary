		<condition>Skin.HasSetting(UseGlobalsearch)</condition>
		<action>RunScript(script.globalsearch)</action>
	</override>

	<override action="partymode_action">
		<condition>MusicPartyMode.Enabled</condition>
		<action>Playercontrol(stop)</action>
	</override>
